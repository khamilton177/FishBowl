<!-- users/index.html.erb -->
<div class="col-sm-8">

<ul>
<% @users.each do |user| %>
  <li>
    <%= link_to("Show #{user.username.capitalize}", user_path(user)) %>
    <%= link_to("Edit", edit_user_path(user), method: :edit) %>
    <%= link_to("Delete", user_path(user), method: :delete) %>
  </li>
<% end %>
</ul>

<%= link_to("New User", new_user_path) %>

</div>
