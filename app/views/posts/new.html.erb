<input class="home" type="hidden" name="current_user" value="<%= @current_user.id %>">

<h1>New Post</h1>
<p>Find me in app/views/posts/new.html.erb</p>

<%= form_for @post do |form| %>
  <%= form.label :title %>
  <%= form.text_field :title %>
  <%= form.label :content %>
  <%= form.text_field :content %>
  <%= form.submit %>
<% end %>

<%= link_to("Back", posts_path) %>
