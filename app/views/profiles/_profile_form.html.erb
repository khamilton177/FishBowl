<%= form.email_field :email, placeholder: "you@somewhere.com", class: "sign_up" %>
<%= form.text_field :fname, placeholder: "First Name", class: "form" %>
<%= form.text_field :lname, placeholder: "Last Name", class: "form" %>
<%= form.date_field :dob, placeholder: "DOB: MM/DD/YYYY", class: "form" %>
<%= form.file_field :avatar, placeholder: "Your Pic", class: "form" %>
<%= form.text_area :about, placeholder: "About me", class: "form" %>
